#: ../deja-dup/preferences/Preferences.vala:262
msgid "_Backup location"
msgstr "_Emplacement de la sauvegarde"

#: ../deja-dup/AssistantRestore.vala:124
msgid "Restore From Where?"
msgstr "Restaurer depuis quel emplacement ?"

#: ../deja-dup/AssistantRestore.vala:146
msgid "_Date"
msgstr "_Date"

#: ../deja-dup/AssistantRestore.vala:169
msgid "Restore files to _original locations"
msgstr "Restaurer les fichiers vers leurs emplacements d'_origines"

#: ../deja-dup/AssistantRestore.vala:174
