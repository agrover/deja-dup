msgid "~/.adobe/Flash_Player/AssetCache"
msgstr ""

#: prefs.page:63(file)
msgid "~/.cache"
msgstr ""

#: prefs.page:64(file)
msgid "~/.gvfs"
msgstr ""

#: prefs.page:65(file)
msgid "~/.Private"
msgstr ""

#: prefs.page:66(file)
msgid "~/.recent-applications.xbel"
msgstr ""

#: prefs.page:67(file)
msgid "~/.recently-used.xbel"
msgstr ""

